{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "ext-curl": "*",
        "laravel/framework": "5.2.*",
        "laravelcollective/html": "5.2.*",
        "cartalyst/sentinel": "2.0.*",
        "cviebrock/eloquent-sluggable": "~3.1.4",
        "cviebrock/eloquent-taggable": "2.0",
        "yajra/laravel-datatables-oracle": "~6.0",
        "geoip2/geoip2": "~2.1",
        "torann/geoip": "1.0.*",
        "alexpechkarev/google-maps": "1.0.5",
        "gregwar/image": "2.*",
        "constantcontact/constantcontact": "2.1.*",
        "laravel/socialite": "^2.0",
        "maatwebsite/excel": "~2.1.0",
        "jcf/geocode": "1.1.*",
        "watson/sitemap": "2.0.*",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "authorizenet/authorizenet": "1.9.2",
        "rap2hpoutre/laravel-credit-card-validator": "^0.0.6",
        "mashape/unirest-php": "3.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "barryvdh/laravel-ide-helper": "^2.1@dev",
        "barryvdh/laravel-debugbar": "^2.0@dev"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Oneweb\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
